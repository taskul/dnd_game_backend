{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "username": {
      "type": "string",
      "minLength": 3,
      "maxLength": 30
    },
    "password": {
      "type": "string",
      "minLength": 6,
      "maxLength": 20
    },
    "first_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "minLength": 6,
      "maxLength": 60,
      "format": "email"
    },
    "guildToken": {
      "type":"string"
    }
  },
  "additionalProperties": false,
  "required": [
    "username",
    "password",
    "email"
  ]
}